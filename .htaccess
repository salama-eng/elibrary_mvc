RewriteEngine On

RewriteCond %{REQUEST_FILE_NAME} !-d
RewriteCond %{REQUEST_FILE_NAME} !-f
RewriteCond %{REQUEST_FILENAME} !.(css|js|png|jpg|jpeg)$
RewriteCond %{REQUEST_FILENAME} !app/assets/adds/.(jpg||jpeg)$
RewriteCond %{REQUEST_FILENAME} !app/assets/icons/.(png)$
RewriteRule ^(.+)$ index.php?url=$1 [QSA,l]